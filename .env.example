# Configuração da API MikroTik
PORT=3000

# Token de autenticação da API (configurar para ativar autenticação)
# Deixe vazio para desativar autenticação
API_TOKEN=your_secure_api_token_here

# Configuração WireGuard
WG_EASY_URL=http://localhost:51821
WG_EASY_PASSWORD=admin
WG_SERVER_PUBLIC_KEY=your_server_public_key_here
WG_SERVER_ENDPOINT=193.181.208.141
WG_SERVER_PORT=51820
WG_PRESHARED_KEY=your_preshared_key_here

# Rate Limiting
# Máximo de requests por minuto por IP do MikroTik
RATE_LIMIT_MAX_REQUESTS=200
RATE_LIMIT_WINDOW_MS=60000